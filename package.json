{
  "name": "un-thanks-project",
  "version": "1.0.0",
  "private": true,
  "description": "UN 참전국 감사 편지 웹 서비스",
  "scripts": {
    "install:all": "npm install && npm run install:frontend && npm run install:backend && npm run install:api",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:api": "node api/install.js",
    "build": "npm run build:frontend && npm run install:api",
    "build:frontend": "cd frontend && npm run build",
    "start": "npm run start:frontend",
    "start:frontend": "cd frontend && npm run preview",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev": "npm run dev:frontend",
    "dev:full": "npx concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:old": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "init-db": "node api/init-db.js",
    "test-api": "node api/test.js",
    "vercel-dev": "vercel dev"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.5.0",
    "mongodb": "^6.16.0",
    "mongoose": "^8.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "vercel": "^32.5.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "korean-war",
    "education",
    "thankfulness",
    "un",
    "letter"
  ]
}
