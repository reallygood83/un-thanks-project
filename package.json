{
  "name": "un-thanks-project",
  "version": "1.0.0",
  "private": true,
  "description": "UN 참전국 감사 편지 웹 서비스",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "start:frontend": "cd frontend && npm run preview",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "init-db": "node api/init-db.js",
    "test-api": "node api/test.js",
    "vercel-dev": "vercel dev"
  },
  "dependencies": {
    "mongodb": "^6.5.0",
    "mongoose": "^8.0.3",
    "uuid": "^11.1.0",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vercel": "^32.5.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "korean-war",
    "education",
    "thankfulness",
    "un",
    "letter"
  ]
}